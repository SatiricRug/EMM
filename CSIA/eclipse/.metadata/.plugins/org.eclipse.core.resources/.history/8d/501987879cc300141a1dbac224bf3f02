package satiric_rug.iamod.config;

import java.io.File;

import net.minecraftforge.common.config.Configuration;

public class ConfigHandler {
	
	public static void init(File configFile) {
		Configuration config = new Configuration(configFile);
		
		try {
			config.load();
			
		} catch (Exception e) {
			
		} finally {
			config.save();
		}
	}
}
